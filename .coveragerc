[run]
source = tools,core,agents
omit =
    */tests/*
    */__init__.py
    */setup.py
    */conftest.py
    */migrations/*
    */venv/*
    */env/*
    */.venv/*
    */.env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    def main\(\):

[html]
directory = coverage_html
title = WitsV3 Test Coverage Report

[xml]
output = coverage.xml 